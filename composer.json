{
    "name": "pbmedia/laravel-browser-kit-macro",
    "description": "A macro to use the Laravel 5.3 testing layer inside your Laravel 5.4+ tests",
    "keywords": [
        "protone-media",
        "laravel-browser-kit"
    ],
    "homepage": "https://github.com/protonemedia/laravel-browser-kit-macro",
    "type": "library",
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/browser-kit-testing": "^5.0 || ^6.0",
        "illuminate/http": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "symfony/dom-crawler": "^4.2 || ^5.2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media"
        }
    ],
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelBrowserKitMacro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelBrowserKitMacro\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\LaravelBrowserKitMacro\\BrowserKitMacroServiceProvider"
            ]
        }
    }
}